<?xml version="1.0" encoding="UTF-8"?>	
<classifiers>
	<classifier id="ibk" className="weka.classifiers.lazy.IBk">
		<parameters>
			<param option="K" description="Set the number of nearest neighbors to use in prediction (default 1)">
				<value>1</value>
				<value>3</value>
				<value>5</value>
			</param>
			<param option="W" description="Set a fixed window size for incremental train/testing. As new training instances are added, oldest instances are removed to maintain the number of training instances at this size. (default no window)">
				<value>0</value>
				<value>20</value>
				<value>50</value>
			</param>
			<param option="*" description="">
				<value>*</value>
				<value>F</value>
				<value>I</value>
			</param>
		</parameters>
	</classifier>
	<classifier id="smo" className="weka.classifiers.functions.SMO">
		<parameters>
			<param option="*">
				<value>*</value>
			</param>
		</parameters>
	</classifier>
</classifiers>