<?xml version="1.0" encoding="UTF-8"?>
<evaluators>
	<evaluator id="CfsSubset" classPath="weka.attributeSelection.CfsSubsetEval">
		<searchMethods>
			<searchMethod refID="bestfirst" />
		</searchMethods>
		<parameters>
			<param option="*"
				description="locallyPredictive -- Identify locally predictive attributes. Iteratively adds attributes with the highest correlation with the class as long as there is not already an attribute in the subset that has a higher correlation with the attribute in question">
				<value></value> <!-- true -->
				<value use="false">L</value> <!-- false -->
			</param>
			<param option="*"
				description="missingSeparate -- Treat missing as a separate value. Otherwise, counts for missing values are distributed across other values in proportion to their frequency.">
				<value></value> <!-- false -->
				<value use="false">M</value> <!-- true -->
			</param>
		</parameters>
	</evaluator>

	<evaluator id="InfoGainAttributeEval" classPath="weka.attributeSelection.InfoGainAttributeEval">
		<searchMethods>
			<searchMethod refID="ranker" />
		</searchMethods>
		<parameters>
			<param option="*">
				<value></value>
			</param>
		</parameters>
		<selected_attributes>
			<value>1%</value>
			<value>2%</value>
			<value>3%</value>
			<value>4%</value>
			<value>5%</value>
			<value>6%</value>
			<value>7%</value>
			<value>8%</value>
			<value>9%</value>
			<value>10%</value>
			<value>30%</value>
			<value>50%</value>
			<value>70%</value>
		</selected_attributes>
	</evaluator>
</evaluators>